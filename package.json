    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "exifr": "^7.1.2",
    "tokenizr": "^1.6.4",
    "tslib": "^1.14.1"
  },
  "prettier": {
    "trailingComma": "all",
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "proseWrap": "always",
    "tabWidth": 2
  }
