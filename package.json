{
  "name": "photo-mover",
  "description": "A cli to group and move photos.",
  "version": "0.0.0",
  "author": "Kyle Pfromer @kpfromer",
  "bin": {
    "photo-mover": "./bin/run"
  },
  "bugs": "https://github.com/kpfromer/photo-mover/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.3.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/kpfromer/photo-mover",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "photo-mover"
  },
  "repository": "kpfromer/photo-mover",
